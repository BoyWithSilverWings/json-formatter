    "eslint": "^6.7.2",
    "file-saver": "^2.0.2",
    "formik": "^2.0.6",
    "lodash": "^4.17.15",
    "monaco-editor": "^0.18.1",
    "peerjs": "^1.1.0",
    "prop-types": "^15.7.2",
    "random-words": "^1.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.0.8",
    "react-spring": "^8.0.27",
    "react-tiny-fab": "^3.2.0",
    "react-use-web-share": "^1.0.1",
    "styled-components": "^4.4.1",
    "whatwg-fetch": "^3.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/scripts/**"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(monaco-editor)/)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agneym/json-formatter"
  }
}
